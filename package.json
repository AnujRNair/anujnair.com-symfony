    "eslint:fix": "eslint --ext .jsx --ext .js src/AnujRNair/AnujNairBundle/Resources/public/js --fix",
    "scss-lint": "sass-lint --config .sass-lint.yml 'src/AnujRNair/AnujNairBundle/Resources/public/**/*.scss' --verbose --no-exit"
