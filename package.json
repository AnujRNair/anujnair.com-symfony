{
  "name": "anujnaircom-symfony",
  "version": "0.0.1",
  "private": true,
  "description": "Dependencies needed to build AnujNair.com via the symfony app",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./web/bundles/assets/ && rm -rf ./node_modules/.cache",
    "build": "webpack --config=./webpack/config.prd.cjs",
    "dev": "webpack-dev-server --hot --config webpack/config.dev.cjs",
    "eslint": "eslint --ext .jsx --ext .js src/AnujRNair/AnujNairBundle/Resources/public/js",
    "eslint:fix": "eslint --ext .jsx --ext .js src/AnujRNair/AnujNairBundle/Resources/public/js --fix",
    "scss-lint": "sass-lint --config .sass-lint.yml 'src/AnujRNair/AnujNairBundle/Resources/public/**/*.scss' --verbose --no-exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnujRNair/anujnair.com-symfony.git"
  },
  "keywords": [
    "anujnair",
    "symfony",
    "webpack"
  ],
  "author": "Anuj Nair <anuj.r.nair@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AnujRNair/anujnair.com-symfony/issues"
  },
  "homepage": "https://github.com/AnujRNair/anujnair.com-symfony#readme",
  "type": "module",
  "devDependencies": {},
  "dependencies": {
    "@babel/core": "^7.17.2",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cache-loader": "^4.1.0",
    "classnames": "^2.3.1",
    "core-js": "^3.21.0",
    "css-loader": "^6.6.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "file-loader": "^6.2.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.5.3",
    "node-sass": "^7.0.1",
    "normalize.css": "^8.0.1",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "prismjs": "^1.26.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass-lint": "^1.13.1",
    "sass-loader": "^12.5.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-manifest-plugin": "^4.1.1",
    "webpack-merge": "^5.8.0"
  }
