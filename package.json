{
  "name": "anujnaircom-symfony",
  "version": "0.0.1",
  "private": true,
  "description": "Dependencies needed to build AnujNair.com via the symfony app",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./web/bundles/assets/ && rm -rf ./node_modules/.cache",
    "build": "webpack --config=./webpack/config.prd.js",
    "dev": "webpack-dev-server --hot --inline --config webpack/config.dev.js",
    "eslint": "eslint --ext .jsx --ext .js src/AnujRNair/AnujNairBundle/Resources/public/js",
    "eslint:fix": "eslint --ext .jsx --ext .js src/AnujRNair/AnujNairBundle/Resources/public/js --fix",
    "scss-lint": "sass-lint --config .sass-lint.yml 'src/AnujRNair/AnujNairBundle/Resources/public/**/*.scss' --verbose --no-exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnujRNair/anujnair.com-symfony.git"
  },
  "keywords": [
    "anujnair",
    "symfony",
    "webpack"
  ],
  "author": "Anuj Nair <anuj.r.nair@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AnujRNair/anujnair.com-symfony/issues"
  },
  "homepage": "https://github.com/AnujRNair/anujnair.com-symfony#readme",
  "devDependencies": {},
  "dependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "autoprefixer": "^9.8.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cache-loader": "^4.1.0",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "css-loader": "^3.6.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^6.0.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.1",
    "normalize.css": "^8.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "prismjs": "^1.26.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass-lint": "^1.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.5",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^4.7.4",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^4.2.2"
  }
